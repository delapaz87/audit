<?phpuse PHPExcel_Reader_Excel2007;$objPHPExcel = new PHPExcel_Reader_Excel2007();$document = $objPHPExcel->load(__DIR__ . '\template\rptinventario.xlsx');$document->getProperties()    ->setCreator("AudIT")    ->setLastModifiedBy("AudIT")    ->setTitle("AudIT Inventario")    ->setSubject("Reporte de Inventario")    ->setDescription("Reporte de Inventario")    ->setCategory("reportes");$i = 1;$cells = 6;foreach ($this->row as $value){    $document->setActiveSheetIndex(0)        ->setCellValue('A'.$cells,$i)        ->setCellValue('B'.$cells,$value['codigo'])        ->setCellValue('C'.$cells,$value['productos'])        ->setCellValue('D'.$cells,$value['marca'])        ->setCellValue('E'.$cells,$value['precio'])        ->setCellValue('F'.$cells,$value['stock'])        ->setCellValue('G'.$cells,$value['fecha_entrada'])        ->setCellValue('H'.$cells,$value['categoria'])        ->setCellValue('I'.$cells,$value['razon_social']);    $i++;    $cells++;}$document->getActiveSheet()->setTitle('Inventario General');$document->setActiveSheetIndex(0);header("Content-Type: application/vnd.openxmlformats-officedocument.spreadsheetml.sheet");header("Content-Disposition: attachment;filename=\"reporte_inventario.xlsx\"");header("Cache-Control: max-age=0");$objWriter = PHPExcel_IOFactory::createWriter($document, 'Excel2007');$objWriter->save('php://output');